---
import Layout from '../layouts/Layout.astro';

import PrimaryCTA from '../components/PrimaryCTA.astro';
import MeetingsFAQ from '../components/MeetingsFAQ.astro';
import IntegrationsMeetings from '../components/IntegrationsMeetings.astro';
import Footer from '../components/Footer.astro';
import Navbar from '../components/Navbar.astro';
import CustomerLogos from '../components/CustomerLogos.astro';
---

<Layout>
  <Navbar />

  <body>
    <!-- Hero Section start -->
    <section class="bg-white dark:bg-gray-900">
        <div class="py-8 px-4 mx-auto max-w-screen-xl lg:py-16 mt-10">
            
            <div class="grid gap-8 items-center mb-8 lg:mb-24 lg:gap-12 lg:grid-cols-12">
                <div class="col-span-6 text-left sm:mb-6 lg:text-left lg:mb-0">
                    <h1 class="mb-4 text-4xl font-extrabold tracking-tight leading-none text-gray-900 md:text-5xl xl:text-6xl dark:text-white">
                        Capture Meeting Insights Across Teams
                    </h1>
                    <p class="mb-6 max-w-xl font-light text-gray-500 xl:mb-8 md:text-lg xl:text-xl dark:text-gray-400">
                        Focus on meaningful conversations and informed decision-making with automatic transcription, summarization, and action item creation from Sindre Notetaker.
                    </p>
                    <form action="#" class="">
                        <div class="space-y-4 sm:flex sm:space-y-0 sm:space-x-4">
                            <div class="relative">
                                <label for="email" class="hidden mb-2 text-sm font-medium text-gray-900 dark:text-gray-300">
                                    Email address
                                </label>
                                <div class="flex absolute inset-y-0 left-0 items-center pl-3 pointer-events-none">
                                    <svg class="w-5 h-5 text-gray-500 dark:text-gray-400" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                                        <path d="M2.003 5.884L10 9.882l7.997-3.998A2 2 0 0016 4H4a2 2 0 00-1.997 1.884z"></path>
                                        <path d="M18 8.118l-8 4-8-4V14a2 2 0 002 2h12a2 2 0 002-2V8.118z"></path>
                                    </svg>
                                </div>
                                <input class="block p-3 pl-10 w-full text-sm text-gray-900 bg-gray-50 rounded-lg border border-gray-300 sm:w-80 xl:w-96 focus:ring-brand-800 focus:border-brand-800 dark:bg-gray-700 dark:border-gray-600 dark:placeholder-gray-400 dark:text-white dark:focus:ring-brand-800 dark:focus:border-brand-800"
                                    placeholder="Enter your email" type="email" id="email" required="">
                            </div>
                            <button type="submit" class="w-full sm:w-auto justify-center text-white bg-brand-800 hover:bg-brand-600 focus:ring-4 focus:outline-none focus:ring-brand-100 font-medium rounded-lg px-5 py-2.5 text-center inline-flex items-center dark:bg-brand-800 dark:hover:bg-brand-600 dark:focus:ring-brand-800">
                                Sign up
                                <svg class="ml-2 -mr-1 w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg">
                                    <path fill-rule="evenodd" d="M10.293 3.293a1 1 0 011.414 0l6 6a1 1 0 010 1.414l-6 6a1 1 0 01-1.414-1.414L14.586 11H3a1 1 0 110-2h11.586l-4.293-4.293a1 1 0 010-1.414z" clip-rule="evenodd"></path>
                                </svg>
                            </button>
                        </div>
                    </form>
                </div>
                <div class="col-span-6 flex justify-end items-center">
                    <img class="rounded-lg shrink-0 w-[400px] h-[400px] relative" style="object-fit: cover" src="/sindre_avatar_with_name_square.png" alt="Sindre" />
                </div>
            </div>

            <!-- Hero's Features Section -->
            <div class="grid lg:grid-cols-4 gap-12">

                <!-- Feature 1 -->
                <div class="grid grid-cols-[auto_1fr]">
                    <div class="flex justify-center items-top w-10 h-10 lg:h-12 lg:w-12">
                        <svg class="w-5 h-5 text-brand-800 lg:w-6 lg:h-6 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 3v4a1 1 0 0 1-1 1H5m4 8h6m-6-4h6m4-8v16a1 1 0 0 1-1 1H6a1 1 0 0 1-1-1V7.914a1 1 0 0 1 .293-.707l3.914-3.914A1 1 0 0 1 9.914 3H18a1 1 0 0 1 1 1Z"/>
                        </svg>
                    </div>
                    <div class="space-y-2">
                        <h3 class="text-xl font-bold dark:text-white">Accurate Multilingual Transcription</h3>
                        <p class="text-gray-500 dark:text-gray-400">Capture every detail with precision in Danish, English, 
                        German, Urdu, and more. Sindre’s advanced technology ensures no important information or nuances are missed.
                        </p>
                    </div>
                </div>

                <!-- Feature 2 -->
                <div class="grid grid-cols-[auto_1fr]">
                    <div class="flex justify-center items-top w-10 h-10 lg:h-12 lg:w-12">
                        <svg class="w-5 h-5 text-brand-800 lg:w-6 lg:h-6 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M16.872 9.687 20 6.56 17.44 4 4 17.44 6.56 20 16.873 9.687Zm0 0-2.56-2.56M6 7v2m0 0v2m0-2H4m2 0h2m7 7v2m0 0v2m0-2h-2m2 0h2M8 4h.01v.01H8V4Zm2 2h.01v.01H10V6Zm2-2h.01v.01H12V4Zm8 8h.01v.01H20V12Zm-2 2h.01v.01H18V14Zm2 2h.01v.01H20V16Z"/>
                        </svg>
                    </div>
                    <div class="space-y-2">
                        <h3 class="text-xl font-bold dark:text-white">Automated Summaries and Action Items</h3>
                        <p class="text-gray-500 dark:text-gray-400">Sindre generates concise summaries of meetings, highlighting 
                        key decisions and automatically assigning action items to the right team members for streamlined follow-ups.
                        </p>
                    </div>
                </div>

                <!-- Feature 3 -->
                <div class="grid grid-cols-[auto_1fr]">
                    <div class="flex justify-center items-top w-10 h-10 lg:h-12 lg:w-12">
                        <svg class="w-5 h-5 text-brand-800 lg:w-6 lg:h-6 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                            <path stroke="currentColor" stroke-width="2" d="M11.083 5.104c.35-.8 1.485-.8 1.834 0l1.752 4.022a1 1 0 0 0 .84.597l4.463.342c.9.069 1.255 1.2.556 1.771l-3.33 2.723a1 1 0 0 0-.337 1.016l1.03 4.119c.214.858-.71 1.552-1.474 1.106l-3.913-2.281a1 1 0 0 0-1.008 0L7.583 20.8c-.764.446-1.688-.248-1.474-1.106l1.03-4.119A1 1 0 0 0 6.8 14.56l-3.33-2.723c-.698-.571-.342-1.702.557-1.771l4.462-.342a1 1 0 0 0 .84-.597l1.753-4.022Z"/>
                        </svg>
                    </div>
                    <div class="space-y-2">
                        <h3 class="text-xl font-bold dark:text-white">Actionable Insights and Cross-Team Collaboration</h3>
                        <p class="text-gray-500 dark:text-gray-400">Gain valuable insights from feedback across sales meetings, 
                        customer interviews, and engineering updates. This holistic view helps you align teams and make informed 
                        decisions without attending every meeting.
                        </p>
                    </div>
                </div>

                <!-- Feature 4 -->
                <div class="grid grid-cols-[auto_1fr]">
                    <div class="flex justify-center items-top w-10 h-10 lg:h-12 lg:w-12">
                        <svg class="w-5 h-5 text-brand-800 lg:w-6 lg:h-6 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 14v3m-3-6V7a3 3 0 1 1 6 0v4m-8 0h10a1 1 0 0 1 1 1v7a1 1 0 0 1-1 1H7a1 1 0 0 1-1-1v-7a1 1 0 0 1 1-1Z"/>
                        </svg>
                    </div>
                    <div class="space-y-2">
                        <h3 class="text-xl font-bold dark:text-white">Secure Integration and Deployment Options</h3>
                        <p class="text-gray-500 dark:text-gray-400">Choose between local or cloud deployment to meet your 
                        security needs, with seamless integration into platforms like Google Meet, Zoom, and Microsoft Teams. 
                        We’re of course GDPR compliant.
                        </p>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- Hero Section end -->
    
  <CustomerLogos />


  <!-- FeaturesOne start -->
    <section class="bg-gray-50 dark:bg-gray-800">
        <div class="items-center py-8 px-4 mx-auto max-w-screen-xl lg:grid xl:grid-cols-3 lg:gap-8 xl:gap-24 sm:py-16 lg:px-6">
            <div class="mb-8 lg:mb-0">
                <h2 class="mb-4 text-4xl tracking-tight font-extrabold text-gray-900 dark:text-white">Highly Accurate Transcription Across Multiple Languages</h2>
                <p class="mb-4 text-gray-500 sm:text-xl dark:text-gray-400">Capture every detail with unmatched accuracy.</p>
            </div>
            <div class="col-span-2 grid grid-cols-2 gap-12">
                <div>
                    <div
                        class="flex justify-center items-center mb-4 w-10 h-10 rounded-full bg-brand-100 lg:h-12 lg:w-12 dark:bg-brand-800">
                        <svg class="w-5 h-5 text-brand-800 lg:w-6 lg:h-6 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M5 14v7M5 4.971v9.541c5.6-5.538 8.4 2.64 14-.086v-9.54C13.4 7.61 10.6-.568 5 4.97Z"/>
                        </svg>
                    </div>
                    <h3 class="mb-2 text-xl font-bold dark:text-white">Multi-Language Support</h3>
                    <p class="text-gray-500 dark:text-gray-400">Sindre offers transcription capabilities in multiple languages, 
                    including Danish, English, German, Urdu, and more, making it suitable for diverse teams and global interactions.
                    </p>
                </div>
                <div>
                    <div
                        class="flex justify-center items-center mb-4 w-10 h-10 rounded-full bg-brand-100 lg:h-12 lg:w-12 dark:bg-brand-800">
                        <svg class="w-5 h-5 text-brand-800 lg:w-6 lg:h-6 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                            <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10.779 17.779 4.36 19.918 6.5 13.5m4.279 4.279 8.364-8.643a3.027 3.027 0 0 0-2.14-5.165 3.03 3.03 0 0 0-2.14.886L6.5 13.5m4.279 4.279L6.499 13.5m2.14 2.14 6.213-6.504M12.75 7.04 17 11.28"/>
                        </svg>
                    </div>
                    <h3 class="mb-2 text-xl font-bold dark:text-white">Accurate Transcription</h3>
                    <p class="text-gray-500 dark:text-gray-400">Our transcription technology outperforms most transcription 
                    services, ensuring that no important information or subtle nuances are missed during discussions.
                    </p>
                </div>
                <div class="col-span-2">
                     <div>
                        <div class="flex justify-center items-center mb-4 w-10 h-10 rounded-full bg-brand-100 lg:h-12 lg:w-12 dark:bg-brand-800">
                        <svg class="w-5 h-5 text-brand-800 lg:w-6 lg:h-6 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                            <path stroke="currentColor" stroke-linecap="round" stroke-width="2" d="M10 3v4a1 1 0 0 1-1 1H5m14-4v16a1 1 0 0 1-1 1H6a1 1 0 0 1-1-1V7.914a1 1 0 0 1 .293-.707l3.914-3.914A1 1 0 0 1 9.914 3H18a1 1 0 0 1 1 1Zm-4 1h.01v.01H15V5Zm-2 2h.01v.01H13V7Zm2 2h.01v.01H15V9Zm-2 2h.01v.01H13V11Zm2 2h.01v.01H15V13Zm-2 2h.01v.01H13V15Zm2 2h.01v.01H15V17Zm-2 2h.01v.01H13V19Z"/>
                        </svg>
                    </div>
                    <h3 class="mb-2 text-xl font-bold dark:text-white">Company-Specific Vocabulary</h3>
                    <p class="text-gray-500 dark:text-gray-400">Sindre effectively captures a wide range of abbreviations and company jargon, 
                    including terms that are often challenging to transcribe accurately. We build a custom vocabulary tailored to each 
                    organization, ensuring that industry-specific language is understood and recorded correctly.
                    </p>
                </div>
            </div>
        </div>
    </section>
    <!-- FeaturesOne end -->

    <!-- FeaturesTwo start -->
    <section class="bg-white dark:bg-gray-900">
        <div
            class="gap-8 items-center py-8 px-4 mx-auto max-w-screen-xl lg:grid lg:grid-cols-2 xl:gap-16 sm:py-16 lg:px-6 ">
            <img class="mb-4 w-full lg:mb-0 rounded-lg"
                src="/meeting-summaries.png"
                alt="summary of meeting">
            <div class="text-gray-500 dark:text-gray-400 sm:text-lg">
                <h2 class="mb-4 text-4xl tracking-tight font-extrabold text-gray-900 dark:text-white">Automated Summaries and Action Items</h2>
                <p class="mb-8 font-light lg:text-xl">Sindre delivers high-quality, human-like summaries and action tiems that provide a complete overview of each meeting.</p>
                <div class="py-8 mb-6 border-t border-b border-gray-200 dark:border-gray-700">
                    <div class="flex">
                        <div
                            class="flex justify-center items-center mr-4 w-8 h-8 rounded-full bg-brand-100 dark:bg-brand-800 shrink-0">
                            <svg class="w-5 h-5 text-brand-800 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M10 3v4a1 1 0 0 1-1 1H5m4 6 2 2 4-4m4-8v16a1 1 0 0 1-1 1H6a1 1 0 0 1-1-1V7.914a1 1 0 0 1 .293-.707l3.914-3.914A1 1 0 0 1 9.914 3H18a1 1 0 0 1 1 1Z"/>
                            </svg>
                        </div>
                        <div>
                            <h3 class="mb-2 text-xl font-bold text-gray-900 dark:text-white">High-Quality Summaries</h3>
                            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">Sindre generates concise, human-like 
                            summaries that provide a comprehensive overview of each meeting, capturing key points and decisions effectively.
                            </p>
                        </div>
                    </div>
                    <div class="flex pt-8">
                        <div
                            class="flex justify-center items-center mr-4 w-8 h-8 bg-brand-100 rounded-full dark:bg-brand-800 shrink-0">
                            <svg class="w-5 h-5 text-brand-800 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 21a9 9 0 1 1 0-18c1.052 0 2.062.18 3 .512M7 9.577l3.923 3.923 8.5-8.5M17 14v6m-3-3h6"/>
                            </svg>
                        </div>
                        <div>
                            <h3 class="mb-2 text-xl font-bold text-gray-900 dark:text-white">Smart Task Assignment</h3>
                            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">Action items are automatically generated 
                            and assigned to the appropriate team members, streamlining follow-ups and ensuring accountability.
                            </p>
                        </div>
                    </div>
                    <div class="flex pt-8">
                        <div
                            class="flex justify-center items-center mr-4 w-8 h-8 bg-brand-100 rounded-full dark:bg-brand-800 shrink-0">
                            <svg class="w-5 h-5 text-brand-800 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 15a6 6 0 1 0 0-12 6 6 0 0 0 0 12Zm0 0v6M9.5 9A2.5 2.5 0 0 1 12 6.5"/>
                            </svg>
                        </div>
                        <div>
                            <h3 class="mb-2 text-xl font-bold text-gray-900 dark:text-white">Increased Accountability</h3>
                            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">With clearly defined action items, 
                            each meeting becomes more actionable, ensuring that all participants know their responsibilities and 
                            next steps after discussions.
                            </p>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </section>
    <!-- FeaturesTwo end -->

    <!-- FeaturesThree start -->
    <section class="bg-white dark:bg-gray-900">
        <div
            class="gap-8 items-center py-8 px-4 mx-auto max-w-screen-xl lg:grid lg:grid-cols-2 xl:gap-16 sm:py-16 lg:px-6 ">
            <div class="text-gray-500 dark:text-gray-400 sm:text-lg">
                <h2 class="mb-4 text-4xl tracking-tight font-extrabold text-gray-900 dark:text-white">Security and Compliance</h2>
                <p class="mb-8 font-light lg:text-xl">Keep your sensitive information secure and compliant.</p>
                <div class="py-8 mb-6 border-t border-b border-gray-200 dark:border-gray-700">
                    <div class="flex">
                        <div
                            class="flex justify-center items-center mr-4 w-8 h-8 rounded-full bg-brand-100 dark:bg-brand-800 shrink-0">
                            <svg class="w-5 h-5 text-brand-800 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M8 20V10m0 10-3-3m3 3 3-3m5-13v10m0-10 3 3m-3-3-3 3"/>
                            </svg>
                        </div>
                        <div>
                            <h3 class="mb-2 text-xl font-bold text-gray-900 dark:text-white">On-Prem or Cloud Deployment</h3>
                            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">You can choose between local or cloud-based 
                            deployment, giving you the flexibility to tailor the solution to fit your security and operational needs.
                            </p>
                        </div>
                    </div>
                    <div class="flex pt-8">
                        <div
                            class="flex justify-center items-center mr-4 w-8 h-8 bg-brand-100 rounded-full dark:bg-brand-800 shrink-0">
                            <svg class="w-5 h-5 text-brand-800 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M21 12a28.076 28.076 0 0 1-1.091 9M7.231 4.37a8.994 8.994 0 0 1 12.88 3.73M2.958 15S3 14.577 3 12a8.949 8.949 0 0 1 1.735-5.307m12.84 3.088A5.98 5.98 0 0 1 18 12a30 30 0 0 1-.464 6.232M6 12a6 6 0 0 1 9.352-4.974M4 21a5.964 5.964 0 0 1 1.01-3.328 5.15 5.15 0 0 0 .786-1.926m8.66 2.486a13.96 13.96 0 0 1-.962 2.683M7.5 19.336C9 17.092 9 14.845 9 12a3 3 0 1 1 6 0c0 .749 0 1.521-.031 2.311M12 12c0 3 0 6-2 9"/>
                            </svg>
                        </div>
                        <div>
                            <h3 class="mb-2 text-xl font-bold text-gray-900 dark:text-white">Secure Data Handling</h3>
                            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">We follow strict security protocols, 
                            including TLS 1.2 and AES-256 encryption, to ensure your data is transmitted and stored securely.
                            </p>
                        </div>
                    </div>
                    <div class="flex pt-8">
                        <div
                            class="flex justify-center items-center mr-4 w-8 h-8 bg-brand-100 rounded-full dark:bg-brand-800 shrink-0">
                            <svg class="w-5 h-5 text-brand-800 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M15 4h3a1 1 0 0 1 1 1v15a1 1 0 0 1-1 1H6a1 1 0 0 1-1-1V5a1 1 0 0 1 1-1h3m0 3h6m-6 7 2 2 4-4m-5-9v4h4V3h-4Z"/>
                            </svg>
                        </div>
                        <div>
                            <h3 class="mb-2 text-xl font-bold text-gray-900 dark:text-white">GDPR Compliance</h3>
                            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">Rest assured, we adhere to GDPR regulations, 
                            so you can trust that all data is handled responsibly and your privacy is protected at all times.
                            </p>
                        </div>
                    </div>
                    <div class="flex pt-8">
                        <div
                            class="flex justify-center items-center mr-4 w-8 h-8 bg-brand-100 rounded-full dark:bg-brand-800 shrink-0">
                            <svg class="w-5 h-5 text-brand-800 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                                <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M9.5 11.5 11 13l4-3.5M12 20a16.405 16.405 0 0 1-5.092-5.804A16.694 16.694 0 0 1 5 6.666L12 4l7 2.667a16.695 16.695 0 0 1-1.908 7.529A16.406 16.406 0 0 1 12 20Z"/>
                            </svg>
                        </div>
                        <div>
                            <h3 class="mb-2 text-xl font-bold text-gray-900 dark:text-white">AI & Data Handling</h3>
                            <p class="mb-2 font-light text-gray-500 dark:text-gray-400">We do not use your meeting data or 
                            any company information to train AI models or LLMs, ensuring your company data remain confidential 
                            and secure.</p>
                        </div>
                    </div>
                </div>
            </div>
            <img class="w-full rounded-lg"
                src="/Security.png"
                alt="GDPR Compliant, Secure Data Handling, & Run it locally">
        </div>
    </section>
    <!-- FeaturesThree end -->

    <!-- Features Four start -->
    <section class="bg-gray-50 dark:bg-gray-800">
        <div class="py-8 px-4 mx-auto max-w-screen-xl sm:py-16 lg:px-6">
            <div class="mx-auto max-w-screen-md text-center mb-8 lg:mb-16">
                <h2 class="mb-4 text-4xl tracking-tight font-extrabold text-gray-900 dark:text-white">Cross-Team Insights and 
                Collaboration
                </h2>
                <p class="font-light text-gray-500 dark:text-gray-400 sm:text-xl">Foster collaboration and shared insights across 
                teams without needing to attend every meeting.
                </p>
            </div>
            <div class="space-y-8 md:grid grid-cols-2 md:gap-8 xl:gap-8 md:space-y-0">
