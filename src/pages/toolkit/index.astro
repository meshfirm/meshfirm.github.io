            <p class="mb-3 text-sm text-gray-600 dark:text-gray-400">User interview question bank</p>
            <span class="text-xs font-medium text-gray-400 dark:text-gray-500">Coming soon</span>
          </div>

        </div>
      </div>
    </section>

    <!-- CTA Section -->
    <section class="bg-white dark:bg-gray-900">
