          <!-- Tool 3 -->
          <div class="p-6 bg-gray-50 rounded-lg dark:bg-gray-800 border border-gray-200 dark:border-gray-700">
            <div class="flex items-center mb-4">
              <div class="flex justify-center items-center w-10 h-10 rounded-lg bg-brand-100 lg:h-12 lg:w-12 dark:bg-brand-800">
                <svg class="w-5 h-5 text-brand-800 lg:w-6 lg:h-6 dark:text-brand-100" aria-hidden="true" xmlns="http://www.w3.org/2000/svg" width="24" height="24" fill="none" viewBox="0 0 24 24">
                  <path stroke="currentColor" stroke-linecap="round" stroke-linejoin="round" stroke-width="2" d="M12 8v4l3 3m6-3a9 9 0 1 1-18 0 9 9 0 0 1 18 0Z"/>
                </svg>
              </div>
              <span class="ml-3 px-2.5 py-0.5 text-xs font-medium bg-gray-200 text-gray-700 rounded dark:bg-gray-700 dark:text-gray-300">Interactive</span>
            </div>
            <h3 class="mb-2 text-xl font-bold text-gray-900 dark:text-white">Sprint Velocity Tracker</h3>
            <p class="mb-4 text-gray-600 dark:text-gray-400">Track your team's velocity over time and forecast delivery with simple charts.</p>
            <span class="inline-flex items-center text-sm font-medium text-gray-400 dark:text-gray-500">Coming soon</span>
          </div>

        </div>
      </div>
    </section>

    <!-- Templates Section -->
    <section id="templates" class="bg-gray-50 dark:bg-gray-800">
