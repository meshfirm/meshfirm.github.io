    document.addEventListener('DOMContentLoaded', function() {
        // Get the tabs container
        const tabsElement = document.getElementById('tabs-example');
        
        // Define the tab elements with their respective content
        const tabElements = [
            {
                id: 'meeting',
                triggerEl: document.getElementById('meeting-tab'),
                targetEl: document.getElementById('meeting-content')
            },
            {
                id: 'sales',
