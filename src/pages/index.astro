        ];
        // Options with default values
        const options = {
            defaultTabId: 'meeting',
            activeClasses: 'text-brand-600 hover:text-brand-600 dark:text-brand-500 dark:hover:text-brand-400 border-brand-600 dark:border-brand-500 bg-white rounded-lg drop-shadow-md',
            inactiveClasses: 'text-gray-500 hover:text-gray-600 dark:text-gray-400 border-transparent hover:border-gray-300 dark:hover:text-gray-300',
            onShow: () => {
                console.log('tab is shown');
            }
        };
        
        // Simple tabs implementation
        function initTabs() {
            // Set initial active tab
            showTab(options.defaultTabId);
            
            // Add click event listeners to each tab
            tabElements.forEach(tab => {
                tab.triggerEl.addEventListener('click', (e) => {
                    e.preventDefault();
                    showTab(tab.id);
                });
            });
        }
        
        // Function to show a specific tab
        function showTab(tabId) {
            // Hide all tab content first
            tabElements.forEach(tab => {
                // Hide tab content
                tab.targetEl.classList.add('hidden');
                
                // Remove active classes from tab trigger
                tab.triggerEl.classList.remove(...options.activeClasses.split(' '));
                tab.triggerEl.classList.add(...options.inactiveClasses.split(' '));
                tab.triggerEl.setAttribute('aria-selected', 'false');
            });
            
            // Find the tab to show
            const activeTab = tabElements.find(tab => tab.id === tabId);
            
            if (activeTab) {
                // Show tab content
                activeTab.targetEl.classList.remove('hidden');
                
                // Add active classes to tab trigger
                activeTab.triggerEl.classList.remove(...options.inactiveClasses.split(' '));
                activeTab.triggerEl.classList.add(...options.activeClasses.split(' '));
                activeTab.triggerEl.setAttribute('aria-selected', 'true');
                
                // Call onShow callback
                if (options.onShow) {
                    options.onShow();
                }
            }
        }
        
        // Initialize the tabs
        initTabs();
    });
    </script>
    <!-- Tabs Script end -->

</Layout>
