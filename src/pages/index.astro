---
import Layout from '../layouts/Layout.astro';

import PrimaryCTA from '../components/PrimaryCTA.astro';
import GeneralFAQ from '../components/GeneralFAQ.astro';
import Integrations from '../components/Integrations.astro';
import Footer from '../components/Footer.astro';
import Navbar from '../components/Navbar.astro';
---

<Layout>
  <Navbar />
  <body>
    <!-- Hero Section start -->
    <section class="bg-white dark:bg-gray-900">
        <div class="py-8 px-4 mx-auto max-w-screen-xl lg:py-16 mt-10">
            <!-- Hero start -->
            <div class="grid gap-8 items-center mb-8 lg:mb-24 lg:gap-12 lg:grid-cols-12">
                <div class="col-span-7 text-left sm:mb-6 lg:text-left lg:mb-0">
                    <h1 class="mb-4 text-4xl font-bold tracking-tight leading-none text-gray-900 md:text-5xl xl:text-6xl dark:text-white">
                        Launch products that truly meet customer needsâ€”faster than ever
                    </h1>
                    <p class="mb-6 max-w-xl font-medium text-gray-900 xl:mb-8 md:text-lg xl:text-xl dark:text-gray-400">
                        Sindre is your digital product manager that captures insights from meetings, uses your preferred tools to build up product sense seamlessly.
                    </p>
                    <button type="submit" class="w-full sm:w-auto justify-center text-white bg-brand-800 hover:bg-brand-600 focus:ring-4 focus:outline-none focus:ring-brand-100 font-medium rounded-lg px-5 py-2.5 text-center inline-flex items-center dark:bg-brand-800 dark:hover:bg-brand-600 dark:focus:ring-brand-800">
